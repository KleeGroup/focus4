{
    "name": "focus4",
    "version": "8.8.0-preview6",
    "description": "Focus v4 (Typescript + MobX)",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/get-focus/focus4.git"
    },
    "author": "damien.frikha@kleegroup.com",
    "license": "MIT",
    "homepage": "https://github.com/get-focus/focus4",
    "scripts": {
        "clean": "node scripts/clean.js",
        "prepublishOnly": "node scripts/clean.js && node scripts/tcm.js && tsc --outDir ./ && node scripts/copy-css.js ./",
        "postinstall": "node scripts/patch-types-react.js",
        "postpublish": "rm -rf config.js config.d.ts index.js index.d.ts ioc.js ioc.d.ts reference.js reference.d.ts user.js user.d.ts util.js util.d.ts && rm -rf application collections components entity layout message network router style testing translation theme",
        "start": "tsc && node scripts/copy-css.js dist",
        "stylelint": "stylelint src/**/*.css",
        "tcm": "node scripts/tcm.js",
        "test": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && tape -r ts-node/register src/entity/__test__/index.ts | tap-spec",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "@types/i18next": "12.1.0",
        "@types/lodash": "4.14.122",
        "@types/numeral": "0.0.25",
        "@types/react": "16.8.7",
        "@types/react-dom": "16.8.2",
        "@types/uuid": "3.4.4",
        "core-decorators": "0.20.0",
        "i18next": "15.0.6",
        "inputmask-core": "2.2.0",
        "inversify": "5.0.1",
        "inversify-inject-decorators": "3.1.0",
        "lodash": "4.17.11",
        "lodash-decorators": "6.0.1",
        "material-design-icons-iconfont": "3.0.3",
        "mobx": "3.6.2",
        "mobx-react": "4.4.3",
        "moment": "2.24.0",
        "numeral": "2.0.6",
        "react": "16.8.4",
        "react-dnd": "7.3.1",
        "react-dnd-html5-backend": "7.2.0",
        "react-dom": "16.8.4",
        "react-pose": "4.0.7",
        "react-toolbox": "2.0.0-beta.13",
        "reflect-metadata": "0.1.10",
        "smoothscroll-polyfill": "0.4.3",
        "tslib": "1.9.3",
        "uuid": "3.3.2",
        "yester": "0.10.0"
    },
    "devDependencies": {
        "@types/shelljs": "0.8.3",
        "@types/tape": "4.2.33",
        "ignore-styles": "5.0.1",
        "shelljs": "0.8.3",
        "stylelint": "9.10.1",
        "tap-spec": "5.0.0",
        "tape": "4.10.1",
        "ts-node": "8.0.3",
        "tslint": "5.13.1",
        "typed-css-modules": "0.3.7",
        "typescript": "3.3.3333"
    }
