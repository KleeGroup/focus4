        "mobx": "4.10.0",
        "mobx-react": "5.4.3",
        "mobx-react-lite": "1.4.0",
        "moment": "2.24.0",
        "moment-timezone": "0.5.25",
        "numeral": "2.0.6",
        "react": "16.8.6",
        "react-dnd": "7.6.0",
        "react-dnd-html5-backend": "7.6.0",
        "react-dom": "16.8.6",
